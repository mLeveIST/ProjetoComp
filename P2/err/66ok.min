program
  number x;
  number y;
  number z
start
  "write 3 numbers:\n"!
  x := ?;
  y := ?;
  z := ?;
  if x <= y then
    if x <= z then
      x! " : is the smallest."!
    else
      x! " : is middle number."!
    fi
  else
    x! " : is larger than "! y! "."!
  fi

  if 1 then
    "\n"!
  fi
end